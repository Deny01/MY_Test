              <DbSource ConnectionRef="happyyf_lyConnectionString (Settings)" DbObjectName="happyYF_ly.dbo.ly_material_plan_main" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.material_plan_num, a.plan_date, a.remark, a.plan_state, a.plan_finished, 
      a.operator, a.sys_date, a.clientname, a.productmodel, a.productnumber, 
      ISNULL(a.plan_preparemateriel, 0) AS plan_preparemateriel, ISNULL(a.is_alertPlan, 
      0) AS is_alertPlan, a.plan_approve, 
      CASE WHEN a.plan_date &gt;= '2017-01-01' THEN isnull(b_1.plan_count, 0) 
      - isnull(c_1.in_qty, 0) ELSE 0 END AS unfinished_count, ISNULL(b_1.oper_dept, '') 
      AS oper_dept
FROM ly_material_plan_main AS a LEFT OUTER JOIN
          (SELECT a.plan_id, b.oper_dept, SUM(a.plan_count) AS plan_count
         FROM ly_material_plan_detail AS a LEFT OUTER JOIN
               ly_inma0010 AS b ON a.wzbh = b.wzbh
         GROUP BY a.plan_id, b.oper_dept) AS b_1 ON 
      a.id = b_1.plan_id LEFT OUTER JOIN
          (SELECT c.plan_id, d.oper_dept, SUM(ISNULL(a.qty, 0)) AS in_qty
         FROM ly_store_in AS a LEFT OUTER JOIN
               ly_production_task_inspection AS b ON 
               a.pruductionTaskInspection_num = b.pruductionTaskInspection_num LEFT OUTER
                JOIN
               ly_production_task AS c ON 
               b.pruductionOrder_num = c.pruductionOrder_num LEFT OUTER JOIN
               ly_inma0010 AS d ON a.wzbh = d.wzbh
         GROUP BY c.plan_id, d.oper_dept) AS c_1 ON b_1.plan_id = c_1.plan_id AND 
      b_1.oper_dept = c_1.oper_dept
WHERE (LEFT(a.material_plan_num, 4) = @jhlb) AND (a.plan_state = 1) AND 
      (a.plan_approve = 1)
ORDER BY a.material_plan_num DESC</CommandText>
