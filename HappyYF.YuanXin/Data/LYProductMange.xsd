                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.plan_id, a.wzbh, ISNULL(a.plan_count, 0) AS plan_count, a.remark, 
      a.operator, a.sys_date, b.mch, b.oper_dept, c.prodname, ISNULL(d.haveassign, 0) 
      AS haveassign, CASE WHEN (ISNULL(a.plan_count, 0) - ISNULL(d .haveassign, 0)) 
      &gt;= 0 THEN (ISNULL(a.plan_count, 0) - ISNULL(d .haveassign, 0)) 
      ELSE 0 END AS noassign, b.xhc, b.gg, ISNULL(e.in_qty, 0) AS finished_count, 
      ISNULL(a.plan_count, 0) - ISNULL(e.in_qty, 0) AS unfinished_count, 
      ISNULL(a.close_count, 0) AS close_count, a.close_peo, a.close_time, 
      COALESCE (a.assemblytime_self, f.Assembly_Time) AS Assembly_Time, 
      COALESCE (a.assemblytime_all, dbo.f_get_asstime(a.wzbh)) AS z_time
FROM ly_material_plan_detail AS a LEFT OUTER JOIN
      ly_inma0010 AS b ON a.wzbh = b.wzbh LEFT OUTER JOIN
      ly_prod_dept AS c ON b.oper_dept = c.prodcode LEFT OUTER JOIN
          (SELECT plan_id, itemno, SUM(make_num) AS haveassign
         FROM ly_production_task
         GROUP BY plan_id, itemno) AS d ON a.plan_id = d.plan_id AND 
      a.wzbh = d.itemno LEFT OUTER JOIN
          (SELECT a.wzbh, c.plan_id, SUM(ISNULL(a.qty, 0)) AS in_qty
         FROM ly_store_in AS a LEFT OUTER JOIN
               ly_production_task_inspection AS b ON 
               a.pruductionTaskInspection_num = b.pruductionTaskInspection_num AND 
               a.machine_num = b.machine_num LEFT OUTER JOIN
               ly_production_task AS c ON 
               b.pruductionOrder_num = c.pruductionOrder_num
         GROUP BY a.wzbh, c.plan_id) AS e ON a.plan_id = e.plan_id AND 
      a.wzbh = e.wzbh LEFT OUTER JOIN
      ly_assembly_time AS f ON a.wzbh = f.Item_Code
WHERE (a.plan_id = @plan_id) AND (c.prodcode = @prodcode)
ORDER BY a.wzbh</CommandText>
