                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.sign_date, a.contract_inner_code, a.contract_code, a.contract_class, 
      a.contract_style, a.salesclient_code, a.submit, a.approve, a.approve_people, 
      a.audit_flag, a.audit_people, a.company_code, a.remark, a.sys_date, c.class_name, 
      d.style_name, e.company_name, b.salesclient_name, b.province, b.city, b.post_code, 
      b.salesclient_address, b.salesclient_phone, b.fax_num, b.bank_name, 
      b.bank_account, b.tax_code, b.supplier_financial_code, b.bank_code, 
      b.salesperson_code, b.salesperson_name, b.salesregion_code, b.salesregion_name, 
      e.phone, e.fax, e.address, e.post_code AS com_post_code, 
      e.bank_name AS com_bank_name, e.bank_account AS com_bank_account, 
      e.tax_code AS com_tax_code, e.legal_person, b.salesclient_contacts, f.con_money, 
      dbo.f_num_chn(f.con_money) AS daxie, a.operator, a.invoice_people, a.invoice_date, 
      ISNULL(a.have_tax, 0) AS have_tax, a.contract_doc_date, a.contract_doc_flag, 
      e.company_name_full, b.client_legal_person, a.approve_date, ISNULL(a.brrow_flag, 
      0) AS brrow_flag, a.out_bill_code, a.directrun_flag, ISNULL(g.deliver_flag, 0) 
      AS deliver_flag, h.out_qty, ISNULL(i.if_give_flag, 0) AS if_give_flag, a.approve_zc, 
      a.approve_people_zc, a.approve_date_zc, f.con_money_zc, 
      dbo.f_num_chn(f.con_money_zc) AS daxieZC, b.salesclient_py, b.salesclient_jp
FROM ly_sales_contract_main AS a LEFT OUTER JOIN
      ly_sales_client_forcontract AS b ON 
      a.contract_code = b.contract_inner_code LEFT OUTER JOIN
      ly_sales_contract_class AS c ON a.contract_class = c.class_code LEFT OUTER JOIN
      ly_sales_contract_style AS d ON a.contract_style = d.style_code LEFT OUTER JOIN
      ly_company_information AS e ON 
      a.company_code = e.company_code LEFT OUTER JOIN
          (SELECT contract_inner_code, SUM(ISNULL(qty, 0) * ISNULL(item_price, 0)) 
               AS con_money, SUM(ISNULL(qty, 0) * ISNULL(item_price, 0) 
               * ISNULL(zc_discount, 80) / 100) AS con_money_zc
         FROM ly_sales_contract_detail AS a
         GROUP BY contract_inner_code) AS f ON 
      a.contract_inner_code = f.contract_inner_code LEFT OUTER JOIN
      ly_material_plan_main AS g ON 
      a.out_bill_code = g.material_plan_num LEFT OUTER JOIN
          (SELECT contract_inner_code, SUM(ISNULL(qty, 0)) AS out_qty
         FROM ly_sales_outbind
         GROUP BY contract_inner_code) AS h ON 
      a.contract_inner_code = h.contract_inner_code LEFT OUTER JOIN
          (SELECT contract_inner_code, CASE WHEN COUNT(*) &gt; 0 THEN CAST(1 AS bit) 
               ELSE CAST(0 AS bit) END AS if_give_flag
         FROM ly_sales_contract_detail AS a
         WHERE (ISNULL(give_flag, 0) = 1)
         GROUP BY contract_inner_code) AS i ON 
      a.contract_inner_code = i.contract_inner_code
WHERE (e.company_name = '中成') AND (ISNULL(a.approve, 0) = 1) AND 
      (a.approve_date &gt;= @begin_date) AND (a.approve_date &lt; @end_date)
ORDER BY a.contract_inner_code DESC</CommandText>
