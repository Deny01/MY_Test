      a.parent_id, a.sales_group_code, a.gitemno, a.gitemname, a.gunit, a.gabsqty, 
      a.gremark, c.xhc AS gxhc, ISNULL(d.pt_qty, 0) AS pt_qty, ISNULL(a.absqty, 0) 
      - ISNULL(d.pt_qty, 0) AS not_qty
FROM ly_plan_getmaterial AS a LEFT OUTER JOIN
      ly_inma0010 AS b ON a.itemno = b.wzbh LEFT OUTER JOIN
      ly_inma0010 AS c ON a.gitemno = c.wzbh LEFT OUTER JOIN
          (SELECT a.itemno AS gitemno, SUM(ISNULL(a.deliver_qty, 0)) AS pt_qty
         FROM ly_sales_deliver_detail AS a LEFT OUTER JOIN
               ly_sales_deliver AS b ON a.deliver_id = b.id
         WHERE (b.group_id = @group_id)
         GROUP BY a.itemno) AS d ON a.gitemno = d.gitemno
WHERE (a.material_plan_num = @material_plan_num) AND 
      (a.sales_group_code = @sales_group_code) AND (a.parent_id &gt; 0)
ORDER BY fir_style, a.parentno, a.itemno</CommandText>
