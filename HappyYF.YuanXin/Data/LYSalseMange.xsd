                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.material_plan_num, a.plan_date, c.contract_inner_code AS remark, 
      a.plan_state, a.plan_finished, a.operator, a.sys_date, 
      CASE WHEN LEFT(c.contract_inner_code, 2) 
      = 'jy' THEN d .salesclient_name ELSE b.salesclient_name END AS clientname, 
      a.productmodel, a.productnumber, a.plan_preparemateriel, a.is_alertPlan, 
      a.is_tempPlan, a.sales_remark, a.tec_people, a.tec_answer_date, a.tec_remark, 
      a.tec_realy_date, a.tec_approve, a.contract_inner_code, a.delivery_date, 
      a.sales_submit, CASE WHEN LEFT(c.contract_inner_code, 2) 
      = 'jy' THEN d .salesperson_name ELSE b.salesperson_name END AS salesperson_name,
       ISNULL(c.approve, 0) AS outstore_flag, a.debug_flag, a.deliver_flag, a.debug_people, 
      a.debug_date, CASE WHEN LEFT(c.contract_inner_code, 2) 
      = 'jy' THEN d .salesclient_py ELSE b.salesclient_py END AS salesclient_py, 
      CASE WHEN LEFT(c.contract_inner_code, 2) 
      = 'jy' THEN d .salesclient_jp ELSE b.salesclient_jp END AS salesclient_jp, 
      a.pro_people, a.pro_date, c.approve_people, c.approve_date, c.remark AS billremark, 
      a.real_delivery_date, a.delivery_remark, c.inner_flag
FROM ly_material_plan_main AS a LEFT OUTER JOIN
      ly_sales_client_forcontract AS b ON 
      a.contract_inner_code = b.contract_inner_code LEFT OUTER JOIN
          (SELECT contract_inner_code, out_bill_code, approve, approve_people, 
               approve_date, remark, inner_flag
         FROM ly_sales_contract_main
         UNION
         SELECT borrow_code AS contract_inner_code, out_bill_code, out_approve, 
               out_approve_people, out_approve_date, remark, 0 AS inner_flag
         FROM ly_sales_borrow) AS c ON 
      a.material_plan_num = c.out_bill_code LEFT OUTER JOIN
      ly_sales_client_forborrow AS d ON c.contract_inner_code = d.borrow_code
WHERE (LEFT(a.material_plan_num, 4) = @jhlb) AND (a.plan_date &gt;= @begindate) AND 
      (a.plan_date &lt; @enddate) AND (a.sales_submit = 1)
ORDER BY a.material_plan_num DESC</CommandText>
