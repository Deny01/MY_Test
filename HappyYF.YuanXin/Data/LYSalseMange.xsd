                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.wzbh, a.mch, a.jph, a.xhc, a.gg, a.dw, a.tyl, a.lys, a.remark, a.sort, 
      CASE WHEN a.status = '成品' THEN a.status ELSE '附件' END AS status, 
      a.warehouse, ISNULL(b.qty, 0) - ISNULL(c.outqty, 0) AS store_count, 
      ISNULL(d.borrowcount, 0) AS borrow_count, b.qty - c.outqty - ISNULL(d.borrowcount, 0) 
      AS real_count, NULL AS planstore_count, CASE WHEN d .mch_py IS NULL 
      THEN dbo.fn_GetPinyin(a.mch) ELSE d .mch_py END AS mch_py, 
      CASE WHEN d .mch_jp IS NULL THEN dbo.fGetPy(a.mch) 
      ELSE d .mch_jp END AS mch_jp, d.sales_price, d.product_discount, 
      ISNULL(d.sec_style, '其它') AS sec_style, (ISNULL(b.qty, 0) - ISNULL(c.outqty, 0) 
      - ISNULL(d.borrowcount, 0)) - (ISNULL(g.in_qty_old, 0) - ISNULL(h.out_qty_old, 0)) 
      AS realstorecount_new
FROM ly_lsptb AS a LEFT OUTER JOIN
      ly_store_insum_View AS b ON a.wzbh = b.wzbh LEFT OUTER JOIN
      ly_store_outall_View AS c ON a.wzbh = c.wzbh LEFT OUTER JOIN
      ly_inma0010 AS d ON a.wzbh = d.wzbh LEFT OUTER JOIN
          (SELECT wzbh, SUM(ISNULL(qty, 0)) AS in_qty_old
         FROM ly_store_in_oldView
         GROUP BY wzbh) AS g ON a.wzbh = g.wzbh LEFT OUTER JOIN
          (SELECT wzbh, SUM(ISNULL(qty, 0)) AS out_qty_old
         FROM ly_store_out_oldView
         GROUP BY wzbh) AS h ON a.wzbh = h.wzbh
WHERE (ISNULL(d.sort1, '') &lt;&gt; '停用') AND (ISNULL(d.warehouse, '') &lt;&gt; '停用')
ORDER BY status, a.wzbh</CommandText>
