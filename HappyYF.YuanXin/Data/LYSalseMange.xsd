                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.borrow_code, a.borrow_date, a.borrow_style, a.borrow_period_begin, 
      a.borrow_period_end, a.borrow_reason, a.contract_code, a.salesclient_code, 
      a.submit, a.out_flag, a.approve, a.approve_people, a.approve_date, a.out_approve, 
      a.out_approve_people, a.out_approve_date, a.operator, a.remark, a.sys_date, 
      b.salesclient_name, b.salesperson_name, c.bumenMC, d.borrow_name, 
      ISNULL(f.deliver_flag, 0) AS deliver_flag, ISNULL(a.directrun_flag, 0) 
      AS directrun_flag, a.out_bill_code, f.contract_inner_code, b.salesregion_name
FROM ly_sales_borrow AS a LEFT OUTER JOIN
      ly_sales_client_forborrow AS b ON 
      a.borrow_code = b.borrow_code LEFT OUTER JOIN
      T_users AS e ON b.salesperson_code = e.yhbm LEFT OUTER JOIN
      T_bumen AS c ON LEFT(e.bumen, 4) = c.bumenID LEFT OUTER JOIN
      ly_sales_borrow_style AS d ON a.borrow_style = d.borrow_code LEFT OUTER JOIN
      ly_material_plan_main AS f ON a.out_bill_code = f.material_plan_num
WHERE (a.salesclient_code = @salesclient_code)
ORDER BY a.borrow_code DESC</CommandText>
