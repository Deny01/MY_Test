      ly_materrial_sort AS d ON c.sort1 = d.sortcode LEFT OUTER JOIN
      ly_store_insum_View AS e ON a_1.gitemno = e.wzbh LEFT OUTER JOIN
      ly_store_outall_View AS f ON a_1.gitemno = f.wzbh LEFT OUTER JOIN
