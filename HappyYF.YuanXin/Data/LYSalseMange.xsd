      - isnull(f.outqty, 0))) &gt; 0 THEN (isnull(a_1.absqty, 0) - (isnull(e.qty, 0) - isnull(f.outqty, 
      0))) ELSE CAST(NULL AS decimal(18, 2)) END AS oweqty, g.clientname, g.pro_people, 
      ISNULL(c_1.out_qty, 0) AS hadborrow_qty, a_1.absqty - ISNULL(c_1.out_qty, 0) 
      AS noborrow_qty
