                    <CommandText>SELECT a.id, ISNULL(a.in_use, 0) AS in_use, a.salesclient_code, a.salesclient_name, 
      a.salesclient_py, a.salesclient_jp, a.province, a.city, a.post_code, 
      a.salesclient_address, a.salesclient_phone, a.fax_num, a.bank_name, 
      a.bank_account, a.tax_code, a.remark, a.supplier_financial_code, a.operator, 
      a.sys_date, a.bank_code, a.salesperson_code, b.salesregion_code, 
      a.salesclient_contacts, b.yhbm, b.yhmc, a.client_legal_person, c.con_money, 
      a.client_discount, a.salesregionSec_code, d.business_count, e.borrow_count, 
      f.receive_count, ISNULL(d.business_count, 0) + ISNULL(e.borrow_count, 0) 
      + ISNULL(f.receive_count, 0) AS all_count, a.warranty, a.category, g.style_name, 
      a.gift_code, h.giftstylename, h.buy_num, h.gift_num, h.execute_time, a.oem_flag, 
      a.salesclient_phone2, a.period_in_scale, a.period_out_scale, a.period_att_in_scale, 
      a.period_att_out_scale, a.salesclient_en_name, a.domain, i.salesregion_name
FROM ly_sales_client AS a LEFT OUTER JOIN
      T_users AS b ON a.salesperson_code = b.yhbm LEFT OUTER JOIN
          (SELECT b.salesclient_code, SUM(ISNULL(a.qty, 0) * ISNULL(a.item_price, 0)) 
               AS con_money
         FROM ly_sales_contract_detail AS a LEFT OUTER JOIN
               ly_sales_contract_main AS b ON 
               a.contract_inner_code = b.contract_inner_code
         WHERE (ISNULL(b.submit, 0) = 1)
         GROUP BY b.salesclient_code) AS c ON 
      a.salesclient_code = c.salesclient_code LEFT OUTER JOIN
          (SELECT salesclient_code, COUNT(*) AS business_count
         FROM ly_sales_business
         GROUP BY salesclient_code) AS d ON 
      a.salesclient_code = d.salesclient_code LEFT OUTER JOIN
          (SELECT salesclient_code, COUNT(*) AS borrow_count
         FROM ly_sales_borrow
         GROUP BY salesclient_code) AS e ON 
      a.salesclient_code = e.salesclient_code LEFT OUTER JOIN
          (SELECT salesclient_code, COUNT(*) AS receive_count
         FROM ly_sales_receive
         GROUP BY salesclient_code) AS f ON 
      a.salesclient_code = f.salesclient_code LEFT OUTER JOIN
      ly_sales_contract_style AS g ON a.category = g.style_code LEFT OUTER JOIN
      ly_sales_gift_style AS h ON a.gift_code = h.giftstylecode LEFT OUTER JOIN
      ly_salesregion AS i ON b.salesregion_code = i.salesregion_code</CommandText>
