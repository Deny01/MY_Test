              a.prod_dept, a.prodname, a.itemno AS gitemno, a.itemname AS gitemname, 
              a.unit, a.status, a.xhc, a.gg, SUM(a.absqty) AS absqty
        FROM ly_plan_getmaterial AS a LEFT OUTER JOIN
              ly_inma0010 AS b ON a.itemno = b.wzbh
        WHERE (a.material_plan_num = @plan_num) AND (ISNULL(b.out, 0) = 1)
        GROUP BY a.plan_id, a.material_plan_num, a.plan_date, a.operator, a.prod_dept, 
              a.prodname, a.itemno, a.itemname, a.unit, a.status, a.xhc, a.gg) 
      AS a_1 LEFT OUTER JOIN
          (SELECT bill_code, out_deptcode, wzbh, SUM(qty) AS out_qty
         FROM ly_store_out
         WHERE (bill_code = @plan_num)
         GROUP BY bill_code, out_deptcode, wzbh) AS b_1 ON 
      a_1.material_plan_num = b_1.bill_code AND a_1.prod_dept = b_1.out_deptcode AND 
      a_1.gitemno = b_1.wzbh LEFT OUTER JOIN
