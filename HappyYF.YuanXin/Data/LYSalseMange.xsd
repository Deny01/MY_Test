      a.tec_answer_date, a.tec_remark, a.tec_realy_date, a.tec_approve, 
      a.contract_inner_code, a.delivery_date, a.sales_submit, b.salesperson_name, 
      c.approve AS outstore_flag, a.debug_flag, a.deliver_flag, 
      CASE WHEN COALESCE (d .bill_code, f.name_billcode) IS NULL THEN CAST(0 AS bit) 
      ELSE CAST(1 AS bit) END AS hadout_flag, CASE WHEN e.bill_code IS NULL 
      THEN CAST(0 AS bit) ELSE CAST(1 AS bit) END AS bjhadout_flag, 
      a.real_delivery_date
FROM ly_material_plan_main AS a LEFT OUTER JOIN
      ly_sales_client_forcontract AS b ON 
      a.contract_inner_code = b.contract_inner_code LEFT OUTER JOIN
          (SELECT contract_inner_code, approve, out_bill_code
         FROM ly_sales_contract_main
         WHERE (contract_code = @contract_inner_code)
         UNION
         SELECT borrow_code AS contract_inner_code, out_flag AS approve, 
               out_bill_code
         FROM ly_sales_borrow
         WHERE (contract_code = @contract_inner_code)) AS c ON 
      a.material_plan_num = c.out_bill_code LEFT OUTER JOIN
          (SELECT DISTINCT bill_code
         FROM ly_store_out
         WHERE (LEFT(wzbh, 2) = 'CP')) AS d ON 
      a.material_plan_num = d.bill_code LEFT OUTER JOIN
