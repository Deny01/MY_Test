                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.wzbh, a.jph, a.mch, a.lb, a.xhc, a.xhj, a.gg, a.dw, a.dj, a.hjc, a.bfl, 
      ISNULL(c.in_qty, 0) - ISNULL(d.out_qty, 0) AS kcl, a.litt, a.bott, a.out, a.tuijian, a.sort1, 
      a.status, a.remark, a.mch_py, a.mch_jp, a.oper_dept, b.prodname, a.warehouse, 
      ISNULL(a.sort_doubt, 0) AS sort_doubt, a.sales_price, a.category, e.sortname, 
      a.work_people, a.fin_lock, a.tec_lock, a.pro_lock, a.series_code, a.series_name, 
      a.commercial_description
FROM ly_inma0010 AS a LEFT OUTER JOIN
      ly_materrial_sort AS e ON e.sortcode = a.sort1 LEFT OUTER JOIN
      ly_prod_dept AS b ON a.oper_dept = b.prodcode LEFT OUTER JOIN
          (SELECT wzbh, SUM(ISNULL(qty, 0)) AS in_qty, MAX(input_date) 
               AS latest_Intime
         FROM ly_store_in
         GROUP BY wzbh) AS c ON a.wzbh = c.wzbh LEFT OUTER JOIN
          (SELECT wzbh, SUM(ISNULL(qty, 0)) AS out_qty, MAX(out_date) 
               AS latest_Outtime
         FROM ly_store_out
         GROUP BY wzbh) AS d ON a.wzbh = d.wzbh
ORDER BY a.wzbh</CommandText>
