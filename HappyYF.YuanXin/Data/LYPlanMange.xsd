      ly_inma0010 AS b ON a.wzbh = b.wzbh LEFT OUTER JOIN
      ly_materrial_sort AS c ON b.sort1 = c.sortcode LEFT OUTER JOIN
          (SELECT d2.id AS plan_id, d1.bill_code, d1.wzbh, SUM(d1.qty) AS in_qty
         FROM ly_store_in AS d1 LEFT OUTER JOIN
               ly_material_plan_main AS d2 ON 
