         WHERE (d2.id = @plan_id)
         GROUP BY d2.id, d1.bill_code, d1.wzbh) AS d ON a.plan_id = d.plan_id AND 
      a.wzbh = d.wzbh LEFT OUTER JOIN
      ly_assembly_time AS f ON a.wzbh = f.Item_Code
WHERE (a.plan_id = @plan_id)</CommandText>
