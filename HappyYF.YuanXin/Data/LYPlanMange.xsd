      &gt; (isnull(a.store_incount, 0) - isnull(a.plan_outcount, 0)) 
      THEN (CASE WHEN (ISNULL(a.plan_outcount, 0) + ISNULL(a.absqty, 0) 
      - ISNULL(a.store_incount, 0)) &gt; 0 THEN (ISNULL(a.plan_outcount, 0) 
      + ISNULL(a.absqty, 0) - ISNULL(a.store_incount, 0)) ELSE 0 END) 
      ELSE (CASE WHEN (ISNULL(a.absqty, 0) - isnull(a.store_realcount, 0)) 
      &gt; 0 THEN (ISNULL(a.absqty, 0) - isnull(a.store_realcount, 0)) ELSE 0 END) END) 
      THEN ISNULL(a.absqty, 0) ELSE (CASE WHEN isnull(a.store_realcount, 0) 
      &gt; (isnull(a.store_incount, 0) - isnull(a.plan_outcount, 0)) 
      THEN (CASE WHEN (ISNULL(a.plan_outcount, 0) + ISNULL(a.absqty, 0) 
      - ISNULL(a.store_incount, 0)) &gt; 0 THEN (ISNULL(a.plan_outcount, 0) 
      + ISNULL(a.absqty, 0) - ISNULL(a.store_incount, 0)) ELSE 0 END) 
      ELSE (CASE WHEN (ISNULL(a.absqty, 0) - isnull(a.store_realcount, 0)) 
      &gt; 0 THEN (ISNULL(a.absqty, 0) - isnull(a.store_realcount, 0)) ELSE 0 END) END) 
      END AS plan_storecount, a.store_realcount, a.newold, a.store_mainteancecount
FROM ly_plan_getmaterial AS a LEFT OUTER JOIN
          (SELECT bill_code, out_deptcode, parentitemno, wzbh, SUM(qty) AS out_qty
         FROM ly_store_out
         WHERE (bill_code = @plan_num)
         GROUP BY bill_code, out_deptcode, parentitemno, wzbh) AS b ON 
      a.material_plan_num = b.bill_code AND a.prod_dept = b.out_deptcode AND 
      a.parentno = b.parentitemno AND a.itemno = b.wzbh LEFT OUTER JOIN
      ly_inma0010 AS c ON a.itemno = c.wzbh LEFT OUTER JOIN
      ly_material_plan_main AS d ON a.plan_id = d.id LEFT OUTER JOIN
      ly_lsptb AS f ON a.ptbid = f.id LEFT OUTER JOIN
          (SELECT bill_code, wzbh, SUM(qty) AS out_qty
         FROM ly_store_in_Maintenance
         WHERE (bill_code = @plan_num)
         GROUP BY bill_code, wzbh) AS g ON a.material_plan_num = g.bill_code AND 
      a.itemno = g.wzbh
WHERE (a.material_plan_num = @plan_num)</CommandText>
