                      <Parameter AllowDbNull="true" AutogeneratedName="qty" ColumnName="qty" DataSourceName="HappyYF_ly.dbo.ly_purchase_contract_append" DataTypeServer="decimal(18, 2)" DbType="Decimal" Direction="Input" ParameterName="@qty" Precision="18" ProviderType="Decimal" Scale="2" Size="9" SourceColumn="qty" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="purchase_qty" ColumnName="purchase_qty" DataSourceName="HappyYF_ly.dbo.ly_purchase_contract_append" DataTypeServer="decimal(18, 2)" DbType="Decimal" Direction="Input" ParameterName="@purchase_qty" Precision="18" ProviderType="Decimal" Scale="2" Size="9" SourceColumn="purchase_qty" SourceColumnNullMapping="false" SourceVersion="Current" />
