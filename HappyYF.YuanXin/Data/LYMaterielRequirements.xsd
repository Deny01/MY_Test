               ly_purchase_contract_main AS c ON 
               a.contract_code = c.contract_code
         WHERE (c.material_plan_num = @material_plan_num)
         GROUP BY a.contract_code, a.itemno) AS e ON 
      a.contract_code = e.contract_code AND a.itemno = e.itemno
WHERE (b.material_plan_num = @material_plan_num)</CommandText>
