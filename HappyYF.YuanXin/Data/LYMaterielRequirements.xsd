                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="contract_code" ColumnName="contract_code" DataSourceName="HappyYF_ly.dbo.ly_purchase_contract_append" DataTypeServer="varchar(20)" DbType="AnsiString" Direction="Input" ParameterName="@contract_code" Precision="0" ProviderType="VarChar" Scale="0" Size="20" SourceColumn="contract_code" SourceColumnNullMapping="false" SourceVersion="Current" />
