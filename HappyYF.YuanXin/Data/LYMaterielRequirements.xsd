      - dbo.f_ly_Contract_InstoreMoney(a.contract_code) AS notinstore_money, c.Phone, 
      c.Phone2, a.audit_flag, a.audit_people, 
      dbo.f_ly_Contract_TotalMoney(a.contract_code) AS cortract_totalmoney, a.final_flag, 
      a.final_people, a.account_period, a.tax_rate, a.freight, a.invoice_date, 
      a.invoice_people, a.invoice_type
FROM ly_purchase_contract_main AS a LEFT OUTER JOIN
      ly_supplier_list_forContract AS b ON a.contract_code = b.contract_code AND 
      a.supplier_code = b.supplier_code LEFT OUTER JOIN
      T_users AS c ON a.buyer = c.yhmc
