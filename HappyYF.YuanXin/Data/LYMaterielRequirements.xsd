                      <Parameter AllowDbNull="true" AutogeneratedName="instore_flag" ColumnName="instore_flag" DataSourceName="HappyYF_ly.dbo.ly_purchase_contract_append" DataTypeServer="bit" DbType="Boolean" Direction="Input" ParameterName="@instore_flag" Precision="0" ProviderType="Bit" Scale="0" Size="1" SourceColumn="instore_flag" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="additional_fee" ColumnName="additional_fee" DataSourceName="HappyYF_ly.dbo.ly_purchase_contract_append" DataTypeServer="decimal(18, 5)" DbType="Decimal" Direction="Input" ParameterName="@additional_fee" Precision="18" ProviderType="Decimal" Scale="5" Size="9" SourceColumn="additional_fee" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </InsertCommand>
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT a.id, a.contract_code, a.itemno, c.supplier_itemno, ISNULL(d.mch, '') AS mch, d.jph, 
      COALESCE (a.xhc, d.xhc, '') AS xhc, COALESCE (a.gg, d.gg, '') AS gg, 
      COALESCE (a.dw, d.dw) AS dw, d.purchase_unit, d.unit_ratio, f.owecount AS qty, 
      c.item_price, c.purchase_lead_time, CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price * a.purchase_qty ELSE a.purchase_qty * c.item_price END AS
       purchase_money, (CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price * a.purchase_qty ELSE a.purchase_qty * c.item_price END)
       / (1 + ISNULL(a.tax_rate, 16) / 100) AS purchase_money_net, 
      CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price ELSE c.item_price END AS contract_price, 
      (CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price ELSE c.item_price END) / (1 + ISNULL(a.tax_rate, 16) 
      / 100) AS contract_price_net, a.remark, a.purchase_qty, COALESCE (a.xhj, d.xhj, '') 
      AS xhj, e.in_qty, (CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price ELSE c.item_price END) * ISNULL(e.in_qty, 0) 
      AS instore_money, ISNULL(a.purchase_qty, 0) - ISNULL(e.in_qty, 0) AS notin_qty, 
      (CASE WHEN isnull(a.contract_price, 0) 
      &gt; 0 THEN a.contract_price ELSE c.item_price END) * (ISNULL(a.purchase_qty, 0) 
      - ISNULL(e.in_qty, 0)) AS notinstore_money, ISNULL(a.id2, 0) AS id2, a.part_name, 
      ISNULL(a.instore_flag, 1) AS instore_flag, ISNULL(a.additional_fee, 0) 
      AS additional_fee, ISNULL(a.additional_fee, 0) / (1 + ISNULL(a.tax_rate, 16) / 100) 
      AS additional_fee_net, a.tax_rate, (CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price * a.purchase_qty ELSE a.purchase_qty * c.item_price END)
       - (CASE WHEN isnull(a.contract_price, 0) 
      &gt;= 0 THEN a.contract_price * a.purchase_qty ELSE a.purchase_qty * c.item_price END)
       / (1 + ISNULL(a.tax_rate, 16) / 100) AS purchase_money_tax, 
      ISNULL(a.additional_fee, 0) - ISNULL(a.additional_fee, 0) / (1 + ISNULL(a.tax_rate, 16) 
      / 100) AS additional_fee_tax, a.arrival_date, dbo.f_LY_Storecount(a.itemno) 
      AS real_storecount, a.ignore_flag, a.ignore_poeple, a.approve_date
FROM ly_purchase_contract_append AS a LEFT OUTER JOIN
      ly_purchase_contract_main AS b ON 
      a.contract_code = b.contract_code LEFT OUTER JOIN
      ly_materiel_supplier AS c ON a.itemno = c.itemno AND 
      b.supplier_code = c.supplier_code AND ISNULL(a.id2, 0) = ISNULL(c.id2, 0) 
      LEFT OUTER JOIN
